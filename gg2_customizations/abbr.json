      "replacement": [
        "<abbr title=\"\\2onfer\">\\1</abbr>"
      ],
      "hint": "Latin cf."
    },
    {
      "match": "\\b(Fig\\.)(\\s+)(\\d+)",
      "replacement": ["<abbr>\\1</abbr>\\2\\3"],
      "hint": "Labeled figures with numeric series"
    },
    {
      "match": "\\b(A\\.\\s*D\\.|B\\.\\s*C\\.|[AP]\\.\\s*M\\.)",
      "replacement": ["<abbr class=\"nowrap spell\">\\1</abbr>"],
      "hint": "A.D., B.C., A.M., P.M.: class=spell"
    },
    {
      "match": "\\b(M\\.\\s*D\\.)",
      "replacement": [
