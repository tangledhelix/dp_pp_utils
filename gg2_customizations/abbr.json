    {
      "match": "\\b((Mr|Mrs|Dr)\\.)",
      "replacement": [
        "<abbr>\\1</abbr>"
      ],
      "hint": "Common titles not needing 'title' attribute"
    },
    {
      "match": "\\b(M\\.)",
      "replacement": [
        "<abbr title=\"Monsieur\" lang=\"fr\">\\1</abbr>"
      ],
      "hint": "Monsieur"
    },
    {
