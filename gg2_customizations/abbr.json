      "replacement": [
        "<abbr title=\"Esquire\">\\1</abbr>"
      ],
      "hint": "Esquire"
    },
    {
      "match": "\\b(Hon\\.)",
      "replacement": [
        "<abbr title=\"Honorable\">\\1</abbr>",
        "<abbr title=\"Honourable\">\\1</abbr>",
        "<abbr title=\"Honorary\">\\1</abbr>"
      ],
      "hint": "'Hon.' titles"
    },
    {
      "match": "\\b((\\d*(1st|2nd|3rd)|\\d+th)\\.?)",
      "replacement": ["<abbr>\\1</abbr>"],
      "hint": "Numeric counting"
    },
    {
